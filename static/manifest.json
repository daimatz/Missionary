{
  "manifest_version": 2,
  "name": "Missionary",
  "description": "Yet Another Slack Notifier for Chrome",
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "notifications",
    "https://slack.com/*"
  ],
  "background": {
    "scripts": ["background-bundle.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "web_accessible_resources": [
    "icon.png"
  ],
  "options_page": "options.html"
}
